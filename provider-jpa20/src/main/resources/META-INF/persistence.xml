<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence
             http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0"
             xmlns="http://java.sun.com/xml/ns/persistence">
    <persistence-unit name="confit-jpa-unit">
        <class>org.deephacks.confit.internal.jpa.JpaProperty</class>
        <class>org.deephacks.confit.internal.jpa.JpaBean</class>
        <class>org.deephacks.confit.internal.jpa.JpaBeanSingleton</class>
        <class>org.deephacks.confit.internal.jpa.JpaRef</class>
        <exclude-unlisted-classes>true</exclude-unlisted-classes>
    </persistence-unit>
</persistence>